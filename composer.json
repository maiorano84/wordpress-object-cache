{
  "name": "maiorano84/wp-object-cache",
  "description": "A Wordpress Object Cache",
  "version": "0.1.0",
  "type": "wordpress-plugin",
  "minimum-stability": "stable",
  "license": "GPL-3.0-or-later",
  "authors": [
    {
      "name": "Matt Maiorano",
      "email": "maiorano84@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.3",
    "psr/simple-cache": "^3.0",
    "psr/cache": "^3.0",
    "symfony/cache": "^7.4"
  },
  "autoload": {
    "psr-4": {
      "Maiorano\\ObjectCache\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Maiorano\\ObjectCache\\Tests\\": "tests"
    }
  },
  "extra": {
    "installer-paths": {
      "web/app/mu-plugins/{$name}/": ["type:wordpress-muplugin"],
      "web/app/plugins/{$name}/": ["type:wordpress-plugin"],
      "web/app/themes/{$name}/": ["type:wordpress-theme"]
    },
    "wordpress-install-dir": "web/wp"
  },
  "require-dev": {
    "phpunit/phpunit": "^12.5"
  }
}
